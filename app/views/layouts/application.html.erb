  </head>
  


  <body class="bck m-0 <%= user_signed_in? ? 'privatebck' : 'publicbck' %>">
  <% if user_signed_in? %>
  <%= render 'home/header' %>
  <% end %>


  <%= yield %>


  <% if notice %>
  <%= render 'layouts/alerts'%>
<% end %>
 

